4,{"$type":".General.Code, LuaSTGEditorSharp","Attributes":[{"attrCap":"Code","attrInput":"function FormatScore(num)\r\n    local str = string.format('%012d', num)\r\n    return string.format('%s,%s,%s,%s',\r\n            str:sub(1,3), str:sub(4,6),str:sub(7,9),str:sub(10,12))\r\nend","EditWindow":"code"}],"AttributeCount":1}
