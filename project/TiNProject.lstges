3,{"$type":".General.Code, LuaSTGEditorSharp","Attributes":[{"attrCap":"Code","attrInput":"function math.lerp(a,b,x)\r\n\treturn a + (b - a) * x\r\nend\r\nfunction GD(e,n,h,l,ex,ph) -- by zino_lath\r\n\tlocal t = {e,n,h,l,ex or 0,ph or 0}\r\n\treturn t[lstg.var.difficulty]\r\nend","EditWindow":"code"}],"AttributeCount":1}
