4,{"$type":".General.Code, LuaSTGEditorSharp","Attributes":[{"attrCap":"Code","attrInput":"function LerpDecel(a, b, x)\r\n    local y = 1 - x\r\n    return (a + (1 - y * y) * (b - a))\r\nend\r\n\r\nfunction Lerp(a, b, t)\r\n    return a + (b - a) * t\r\nend","EditWindow":"code"}],"AttributeCount":1}
